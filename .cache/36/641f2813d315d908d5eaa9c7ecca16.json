"line":186,"column":2}},{"source":"scss/main.scss","name":null,"original":{"line":20,"column":2},"generated":{"line":187,"column":2}}],"sources":{"scss/main.scss":"/* for all imports */\n@use \"./abstracts/g-abstracts\";\n@use \"./base/g-base\";\n@use \"./components/g-components\";\n@use \"./layout/g-layout\";\n\n.main-contain {\n  display: flex;\n  flex-direction: row;\n}\nmain {\n  flex: 3;\n  padding-top: 150px;\n}\n\nsection {\n  margin: 10px;\n  padding: 20px;\n  background-color: white;\n  border-radius: 5px;\n}\n","scss/abstracts/_g-abstracts.scss":"/* hier importieren wir unsere links */\n/* abstracts/_global.scss */\n\n@use \"./variables\";\n@use \"./mixins\";\n","scss/abstracts/_mixins.scss":"/* list for nav */\n@mixin reset-list {\n  margin: 0;\n  padding: 0;\n  list-style: none;\n}\n\n@mixin horizontal-list {\n  @include reset-list;\n  li {\n    display: inline-block;\n    margin: {\n      left: -5px;\n      right: 1rem;\n    }\n  }\n}\n\n/* font-size */\n@mixin font-size-global($font-s: 1rem) {\n  font-size: $font-s;\n\n  @if $font-s < 2rem {\n    line-height: 1.5;\n  } @else if $font-s >= 2rem and $font-s < 3rem {\n    line-height: 1.3;\n  } @else if $font-s >= 3rem and $font-s < 5rem {\n    line-height: 1.2;\n  } @else {\n    line-height: 1;\n  }\n}\n","scss/base/_g-base.scss":"/* @use \"../abstracts/globals\"; */\n@use \"./fonts\";\n@use \"./reset\";\n/* @use \"./general\"; */\n","scss/base/_fonts.scss":"@use \"../abstracts/mixins\" as *;\n$font-type: Arial, sans-serif;\n$font-size: 16px;\n$font-style: oblique;\n\nh1 {\n  @include font-size-global(4rem);\n  padding: 0.5rem 0;\n}\n\nh2 {\n  @include font-size-global(3rem);\n  padding: 0.5rem 0;\n}\nh3 {\n  @include font-size-global(2rem);\n  padding: 0.5rem 0;\n}\np {\n  @include font-size-global(1.2rem);\n  padding: 0.25rem 0;\n}\n","scss/base/_reset.scss":"* {\n  margin: 0;\n  padding: 0;\n  box-sizing: border-box;\n  font-size: 14px;\n}\nbody,\nhtml {\n  padding: 1.5rem;\n  line-height: 24px;\n}\n","scss/components/_tables.scss":"table {\n  border: 1px solid #ddd;\n  th {\n    background-color: #f4f4f4;\n  }\n  th,\n  td {\n    border: 1px solid #ddd;\n    padding: 10px;\n    text-align: left;\n  }\n}\n","scss/components/_list.scss":"#skills ul {\n  list-style-type: none;\n  padding: 0;\n  background-color: green;\n\n  li {\n    margin: 10px 0;\n    padding: 10px;\n    background-color: #fff;\n    border: 1px solid #ddd;\n    border-radius: 5px;\n    span {\n      display: block;\n    }\n  }\n}\n","scss/layout/_body.scss":"@use \"../abstracts/variables\" as *;\n@use \"../base/fonts\" as *;\n\nbody {\n  font-family: $font-type;\n  font-size: $font-size;\n  font-style: $font-style;\n  margin: 0;\n  padding: 0;\n  background-color: $bg-color;\n  color: $text-color;\n}\n","scss/abstracts/_variables.scss":"// variables for projects\n\n$text-color: #333;\n$first-color: #a5bbae9c;\n$second-color: #8b9af6;\n$bg-color: #f4f4f4;\n","scss/layout/_nav.scss":"@use \"../abstracts/mixins\" as *;\n\nnav ul {\n  @include horizontal-list;\n  a {\n    text-decoration: none;\n  }\n}\n","scss/layout/_header.scss":"header {\n  background-color: #aaa;\n  color: white;\n  text-align: center;\n  position: fixed;\n  top: 0;\n  left: 0;\n  z-index: 10;\n  width: 100%;\n\n  /*  */\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 25px 15px;\n\n  .logo {\n    height: 100px;\n    flex: 1;\n  }\n  nav {\n    flex: 2;\n  }\n  margin-bottom: 100px;\n  p {\n    margin: 10px 0 0;\n  }\n}\n","scss/layout/_footer.scss":"footer {\n  width: 100%;\n  height: 10rem;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  gap: 30px;\n}\n","scss/layout/_forms.scss":"@use \"../components/buttons\" as *;\n\nform {\n  display: flex;\n  flex-direction: column;\n  margin-bottom: 15px;\n\n  label {\n    margin-top: 10px;\n  }\n\n  input,\n  textarea {\n    margin-top: 5px;\n    padding: 10px;\n    font-size: 16px;\n    border: 1px solid #ddd;\n    border-radius: 3px;\n  }\n\n  button {\n    @include btn();\n  }\n}\n","scss/components/_buttons.scss":"@mixin btn($color: black, $bg-color: green) {\n  text-decoration: none;\n  cursor: pointer;\n  border: 0;\n  padding: 1rem 1.5rem;\n  color: $color;\n  background-color: $bg-color;\n  margin: 1rem 0;\n\n  &:hover,\n  &:focus {\n    background-color: lightgreen;\n  }\n}\n","scss/layout/_aside.scss":"@use \"../abstracts/variables\" as *;\n@use \"../abstracts/mixins\" as *;\n\naside {\n  flex: 1;\n  padding-top: 150px;\n  margin: 10px;\n  padding: 20px;\n  background-color: white;\n  border-radius: 5px;\n}\n"},"lineCount":null}},"error":null,"hash":"b767825c07743280934ef291a2534450","cacheData":{"env":{}}}